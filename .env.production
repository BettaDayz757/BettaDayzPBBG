# Production deployment configuration for BettaDayz PBBG
NODE_ENV=production
NEXT_PUBLIC_SITE_URL=https://bettadayz.shop
NEXT_PUBLIC_SITE_NAME=BettaDayz PBBG

# Supabase (replace with your production values)
NEXT_PUBLIC_SUPABASE_URL=your_production_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_production_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_production_service_role_key

# Payment Processing (production keys)
STRIPE_SECRET_KEY=sk_live_your_stripe_secret_key
STRIPE_PUBLISHABLE_KEY=pk_live_your_stripe_publishable_key
STRIPE_WEBHOOK_SECRET=whsec_your_production_webhook_secret

CASHAPP_CASHTAG=$BettaDayzGame
BITCOIN_ADDRESS=your_production_bitcoin_address

# External APIs
COINBASE_API_KEY=your_coinbase_api_key
BLOCKCHAIN_INFO_API_KEY=your_blockchain_api_key

# Security
NEXTAUTH_SECRET=your_secure_nextauth_secret
NEXTAUTH_URL=https://bettadayz.shop

# Analytics
GOOGLE_ANALYTICS_ID=your_ga_tracking_id

# Session Secret (generate a secure random string)
# SESSION_SECRET=your_session_secret_here

# Other configuration
REMIX_DEV_SERVER_WS_PORT=8002